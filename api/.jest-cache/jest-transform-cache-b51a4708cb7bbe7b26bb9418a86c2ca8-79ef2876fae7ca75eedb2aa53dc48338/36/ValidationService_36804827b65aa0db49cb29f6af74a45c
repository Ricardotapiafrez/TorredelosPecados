        cov_17a5moggrc().s[81]++;
        if (cardIndex >= player.hand.length) {
          /* istanbul ignore next */
          cov_17a5moggrc().b[18][0]++;
          cov_17a5moggrc().s[82]++;
          result.errors.push('Carta no disponible en la mano');
          /* istanbul ignore next */
          cov_17a5moggrc().s[83]++;
          result.isValid = false;
        } else {
          /* istanbul ignore next */
          cov_17a5moggrc().b[18][1]++;
          cov_17a5moggrc().s[84]++;
          result.warnings.push('Fase de la Mano: Robarás una carta del Pozo de Almas');
        }
        /* istanbul ignore next */
        cov_17a5moggrc().s[85]++;
        break;
      case 'faceUp':
        /* istanbul ignore next */
        cov_17a5moggrc().b[17][1]++;
        cov_17a5moggrc().s[86]++;
        if (cardIndex >= player.faceUpCreatures.length) {
          /* istanbul ignore next */
          cov_17a5moggrc().b[19][0]++;
          cov_17a5moggrc().s[87]++;
          result.errors.push('Carta no disponible en criaturas boca arriba');
          /* istanbul ignore next */
          cov_17a5moggrc().s[88]++;
          result.isValid = false;
        } else {
          /* istanbul ignore next */
          cov_17a5moggrc().b[19][1]++;
          cov_17a5moggrc().s[89]++;
          result.warnings.push('Fase de Criaturas Boca Arriba: No puedes robar del Pozo de Almas');
        }
        /* istanbul ignore next */
        cov_17a5moggrc().s[90]++;
        break;
      case 'faceDown':
        /* istanbul ignore next */
        cov_17a5moggrc().b[17][2]++;
        cov_17a5moggrc().s[91]++;
        if (cardIndex >= player.faceDownCreatures.length) {
          /* istanbul ignore next */
          cov_17a5moggrc().b[20][0]++;
          cov_17a5moggrc().s[92]++;
          result.errors.push('Carta no disponible en criaturas boca abajo');
          /* istanbul ignore next */
          cov_17a5moggrc().s[93]++;
          result.isValid = false;
        } else {
          /* istanbul ignore next */
          cov_17a5moggrc().b[20][1]++;
          cov_17a5moggrc().s[94]++;
          result.warnings.push('Fase de Criaturas Boca Abajo: Si la carta es inválida, tomarás la Torre de los Pecados');

          // Verificar si la carta podría ser inválida
          /* istanbul ignore next */
          cov_17a5moggrc().s[95]++;
          if (
          /* istanbul ignore next */
          (cov_17a5moggrc().b[22][0]++, game.towerOfSins.lastPlayedCard) &&
          /* istanbul ignore next */
          (cov_17a5moggrc().b[22][1]++, card) &&
          /* istanbul ignore next */
          (cov_17a5moggrc().b[22][2]++, !card.isSpecial)) {
            /* istanbul ignore next */
            cov_17a5moggrc().b[21][0]++;
            const towerValidation =
            /* istanbul ignore next */
            (cov_17a5moggrc().s[96]++, game.towerOfSins.canPlayCard(card));
            /* istanbul ignore next */
            cov_17a5moggrc().s[97]++;
            if (!towerValidation.canPlay) {
              /* istanbul ignore next */
              cov_17a5moggrc().b[23][0]++;
              cov_17a5moggrc().s[98]++;
              result.warnings.push(`⚠️ Esta carta (${card.value}) - ${towerValidation.reason} - tomarás la Torre de los Pecados`);
            } else
            /* istanbul ignore next */
            {
              cov_17a5moggrc().b[23][1]++;
            }
          } else
          /* istanbul ignore next */
          {
            cov_17a5moggrc().b[21][1]++;
          }
        }
        /* istanbul ignore next */
        cov_17a5moggrc().s[99]++;
        break;
      default:
        /* istanbul ignore next */
        cov_17a5moggrc().b[17][3]++;
        cov_17a5moggrc().s[100]++;
        result.errors.push('Fase de juego no válida');
        /* istanbul ignore next */
        cov_17a5moggrc().s[101]++;
        result.isValid = false;
    }
    /* istanbul ignore next */
    cov_17a5moggrc().s[102]++;
    return result;
  }

  // Validar purificación
  validatePurification(game, card) {
    /* istanbul ignore next */
    cov_17a5moggrc().f[7]++;
    const result =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[103]++, {
      willPurify: false,
      message: ''
    });
    /* istanbul ignore next */
    cov_17a5moggrc().s[104]++;
    if (card.willPurifyPile(game)) {
      /* istanbul ignore next */
      cov_17a5moggrc().b[24][0]++;
      cov_17a5moggrc().s[105]++;
      result.willPurify = true;
      /* istanbul ignore next */
      cov_17a5moggrc().s[106]++;
      result.message = 'Esta carta purificará la Torre de los Pecados';
    } else
    /* istanbul ignore next */
    {
      cov_17a5moggrc().b[24][1]++;
    }
    cov_17a5moggrc().s[107]++;
    return result;
  }

  // Obtener todas las cartas jugables para un jugador
  getPlayableCardsForPlayer(game, playerId) {
    /* istanbul ignore next */
    cov_17a5moggrc().f[8]++;
    const player =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[108]++, game.players.find(p => {
      /* istanbul ignore next */
      cov_17a5moggrc().f[9]++;
      cov_17a5moggrc().s[109]++;
      return p.id === playerId;
    }));
    /* istanbul ignore next */
    cov_17a5moggrc().s[110]++;
    if (!player) {
      /* istanbul ignore next */
      cov_17a5moggrc().b[25][0]++;
      cov_17a5moggrc().s[111]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_17a5moggrc().b[25][1]++;
    }
    const playableCards =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[112]++, player.getPlayableCards());
    /* istanbul ignore next */
    cov_17a5moggrc().s[113]++;
    return playableCards.filter(card => {
      /* istanbul ignore next */
      cov_17a5moggrc().f[10]++;
      cov_17a5moggrc().s[114]++;
      return card.canBePlayed(game, playerId);
    });
  }

  // Verificar si un jugador puede jugar (tiene cartas jugables)
  canPlayerPlay(game, playerId) {
    /* istanbul ignore next */
    cov_17a5moggrc().f[11]++;
    const playableCards =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[115]++, this.getPlayableCardsForPlayer(game, playerId));
    /* istanbul ignore next */
    cov_17a5moggrc().s[116]++;
    return playableCards.length > 0;
  }

  // Obtener información de validación para el frontend
  getValidationInfo(game, playerId) {
    /* istanbul ignore next */
    cov_17a5moggrc().f[12]++;
    const player =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[117]++, game.players.find(p => {
      /* istanbul ignore next */
      cov_17a5moggrc().f[13]++;
      cov_17a5moggrc().s[118]++;
      return p.id === playerId;
    }));
    /* istanbul ignore next */
    cov_17a5moggrc().s[119]++;
    if (!player) {
      /* istanbul ignore next */
      cov_17a5moggrc().b[26][0]++;
      cov_17a5moggrc().s[120]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_17a5moggrc().b[26][1]++;
    }
    const playableCards =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[121]++, this.getPlayableCardsForPlayer(game, playerId));
    const canPlay =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[122]++, playableCards.length > 0);
    const isCurrentTurn =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[123]++, game.players[game.currentPlayerIndex]?.id === playerId);
    /* istanbul ignore next */
    cov_17a5moggrc().s[124]++;
    return {
      canPlay,
      playableCards: playableCards.map(card => {
        /* istanbul ignore next */
        cov_17a5moggrc().f[14]++;
        cov_17a5moggrc().s[125]++;
        return card.getPublicInfo();
      }),
      currentPhase: player.currentPhase,
      handSize: player.hand.length,
      faceUpSize: player.faceUpCreatures.length,
      faceDownSize: player.faceDownCreatures.length,
      soulWellSize: player.soulWell.length,
      isCurrentTurn,
      turnInfo: game.getTurnInfo(),
      nextPlayerCanPlayAnything: game.nextPlayerCanPlayAnything,
      lastPlayedCard: game.towerOfSins.lastPlayedCard ?
      /* istanbul ignore next */
      (cov_17a5moggrc().b[27][0]++, game.towerOfSins.lastPlayedCard.getPublicInfo()) :
      /* istanbul ignore next */
      (cov_17a5moggrc().b[27][1]++, null),
      discardPileSize: game.towerOfSins.cards.length,
      shouldTakeDiscardPile: this.shouldTakeDiscardPile(game, playerId),
      turnValidation: isCurrentTurn ?
      /* istanbul ignore next */
      (cov_17a5moggrc().b[28][0]++, this.validateTurn(game, playerId)) :
      /* istanbul ignore next */
      (cov_17a5moggrc().b[28][1]++, null)
    };
  }

  // Validar si un jugador debe tomar la Torre de los Pecados
  shouldTakeDiscardPile(game, playerId) {
    /* istanbul ignore next */
    cov_17a5moggrc().f[15]++;
    const player =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[126]++, game.players.find(p => {
      /* istanbul ignore next */
      cov_17a5moggrc().f[16]++;
      cov_17a5moggrc().s[127]++;
      return p.id === playerId;
    }));
    /* istanbul ignore next */
    cov_17a5moggrc().s[128]++;
    if (!player) {
      /* istanbul ignore next */
      cov_17a5moggrc().b[29][0]++;
      cov_17a5moggrc().s[129]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_17a5moggrc().b[29][1]++;
    }

    // Si el jugador no tiene cartas jugables, debe tomar la Torre
    cov_17a5moggrc().s[130]++;
    return /* istanbul ignore next */(cov_17a5moggrc().b[30][0]++, !this.canPlayerPlay(game, playerId)) &&
    /* istanbul ignore next */
    (cov_17a5moggrc().b[30][1]++, game.towerOfSins.cards.length > 0);
  }

  // Obtener estadísticas de validación
  getValidationStats(game) {
    /* istanbul ignore next */
    cov_17a5moggrc().f[17]++;
    const stats =
    /* istanbul ignore next */
    (cov_17a5moggrc().s[131]++, {
      totalPlayers: game.players.length,
      playersWithPlayableCards: 0,
      playersWhoMustTakePile: 0,
      currentTurnPlayer:
      /* istanbul ignore next */
      (cov_17a5moggrc().b[31][0]++, game.players[game.currentPlayerIndex]?.id) ||
      /* istanbul ignore next */
      (cov_17a5moggrc().b[31][1]++, null),
      gamePhase: game.gameState,
      discardPileSize: game.discardPile.length,
      lastPlayedCard: game.lastPlayedCard ?
      /* istanbul ignore next */
      (cov_17a5moggrc().b[32][0]++, game.lastPlayedCard.name) :
      /* istanbul ignore next */
      (cov_17a5moggrc().b[32][1]++, null),
      nextPlayerCanPlayAnything: game.nextPlayerCanPlayAnything,
      skippedPlayer: game.skippedPlayer
    });
    /* istanbul ignore next */
    cov_17a5moggrc().s[132]++;
    game.players.forEach(player => {
      /* istanbul ignore next */
      cov_17a5moggrc().f[18]++;
      cov_17a5moggrc().s[133]++;
      if (this.canPlayerPlay(game, player.id)) {
        /* istanbul ignore next */
        cov_17a5moggrc().b[33][0]++;
        cov_17a5moggrc().s[134]++;
        stats.playersWithPlayableCards++;
      } else
      /* istanbul ignore next */
      {
        cov_17a5moggrc().b[33][1]++;
      }
      cov_17a5moggrc().s[135]++;
      if (this.shouldTakeDiscardPile(game, player.id)) {
        /* istanbul ignore next */
        cov_17a5moggrc().b[34][0]++;
        cov_17a5moggrc().s[136]++;
        stats.playersWhoMustTakePile++;
      } else
      /* istanbul ignore next */
      {
        cov_17a5moggrc().b[34][1]++;
      }
    });
    /* istanbul ignore next */
    cov_17a5moggrc().s[137]++;
    return stats;
  }
}
